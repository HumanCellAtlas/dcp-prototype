{
  "Version": "2012-10-17",
  "Statement": [
    {
        "Effect": "Allow",
        "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents"
        ],
        "Resource": "arn:aws:logs:*:*:*"
    },
    {
        "Sid": "CloudFront",
        "Effect": "Allow",
        "Action": [
            "cloudfront:ListDistributions",
            "cloudfront:CreateInvalidation"
        ],
        "Resource": "*"
    }
  ]
}
